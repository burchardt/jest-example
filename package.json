{
  "name": "jest-example",
  "version": "1.0.0",
  "description": "Simple jest example",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "^0.9.5",
    "validator": "^7.0.0",
    "webpack": "1.14.0"
  },
  "devDependencies": {
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.0",
    "react-test-renderer": "^15.4.2"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --content-base src --inline --hot",
    "start": "react-scripts start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules/",
      "./public"
    ]
  },
